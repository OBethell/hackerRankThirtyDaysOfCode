import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {
    
    // Method to produce a factorial of an int
    static int factorial(int n) {
        // Cycle through all positive non-zero intergers lower than the input.
        for(int x = n-1; x > 0; x--){
            n *= x;
        }
        return n;
    }

    public static void main(String[] args) {
        
        // Initalise scanner
        Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        // Print factorical
        System.out.println(factorial(n));
    }
}
